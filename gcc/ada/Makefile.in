TOOLS_FLAGS_TO_PASS=		\
        "CC=$(CC)" 		\
        "CFLAGS=$(CFLAGS)"	\
	"LDFLAGS=$(LDFLAGS)"	\
        "ADAFLAGS=$(ADAFLAGS)"	\
        "INCLUDES=$(INCLUDES_FOR_SUBDIR)"\
