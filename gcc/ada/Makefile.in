	  --GNATBIND="$(GNATBIND)" --GNATLINK="$(GNATLINK)" \
	  -nostdlib $(fsrcpfx)gnatlbr -o $@ \
	  -largs --GCC="$(CC) $(ALL_CFLAGS) $(LDFLAGS)" \
	  $(GNATLBR_C_OBJS) $(EXTRA_GNATTOOLS_OBJS)

